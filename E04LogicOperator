package ex03operator;

public class E04LogicOperator
{

	public static void main(String[] args)
	{
		/*
		논리연산자
		&& : 논리 And, 양쪽 모두가 true일때만 true를 반환하고,
		나머지는 false를 반환하는 연산자
		|| 논리 Or, 한쪽만 true 이면 true를 반환하고 , 양쪽 모두 false 일때만
		false를 반환하는 연산자
		!: 논리 Not. 반대의 논리를 반환하는 연산자
		 */
		int num1=10, num2=20;
		
		// false && true > false 반환 합니다 (예 : 사이트 로그인, 비밀번호)
		boolean result1 = (num1 == 100 && num2 ==20);
		
		// true || false > 한쪽만 true 여도 true 이므로 true 반환
		boolean result2 = (num1<12 || num2>=30);
		
		System.out.println("result1의결과:"+ result1);
		System.out.println("result1의결과:"+ result2);
		
		//!(false) > true
		//if문의 조건이 true 이므로 첫 번째 블록을 실행한다.
		if(!(num1==num2)) {
			System.out.println("넘1과 num2는 다르다");
			
		}else {
			System.out.println("num1과 num2는 같다");
			
		}
		
	}

}
